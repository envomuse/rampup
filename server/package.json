{
  "name": "envmuse-server",
  "version": "0.1.0",
  "description": "envmuse server",
  "main": "index.js",
  "author": "wangren.wr",
  "private": false,
  "scripts": {
    "babel-node": "babel-node index.js",
    "server": "cross-env PORT=3004 nodemon --ignore dbfiles --ignore webapps --ignore views --exec npm run babel-node",
    "proxyserver": "cross-env PROXY=* nodemon --ignore dbfiles --ignore webapps/ --ignore views/ --exec babel-node index.js",
    "pm2start": "cross-env PORT=3004 pm2 start entry.js --name='sic3shell'",
    "pm2stop": "pm2 delete 'sic3shell'",
    "pm2list": "pm2 list",
    "echoDB": "babel-node scripts/echodb.js",
    "changeUserPassword": "babel-node scripts/change_user_password.js"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "babel-register": "^6.18.0",
    "co": "~4.6.0",
    "co-body": "~4.0.0",
    "co-busboy": "~1.3.1",
    "co-nedb": "^2.0.1",
    "co-request": "~1.0.0",
    "commander": "^2.9.0",
    "fs-extra": "~0.26.4",
    "immutable": "~3.7.6",
    "koa-cors": "0.0.16",
    "koa-generic-session": "1.10",
    "koa-logger": "~1.3.0",
    "koa-mount": "^1.3.0",
    "koa-no-cache": "~1.1.0",
    "koa-proxy": "^0.4.1",
    "koa-router": "^5.3.0",
    "koa-safe-jsonp": "~0.3.1",
    "koa-static": "~2.0.0",
    "koa-static-cache": "3.1.2",
    "koa-views": "~3.1.0",
    "koa.io": "0.0.3",
